import 'dart:io';

main() {
  double a = input();
  double b = input();

  display(
      add: add(a: a, b: b),
      subtract: subtract(a: a, b: b),
      multiply: multiply(a: a, b: b),
      divide: divide(a: a, b: b));
}

double input() {
  stdout.write('Enter number: ');
  double input = double.parse(stdin.readLineSync()!);
  return input;
}

double add({required double a, required double b}) {
  return a + b;
}

double subtract({required double a, required double b}) {
  return a - b;
}

double multiply({required double a, required double b}) {
  return a * b;
}

double divide({required double a, required double b}) {
  return a / b;
}

void display(
    {required add, required subtract, required multiply, required divide}) {
  print(add);
  print(subtract);
  print(multiply);
  print(divide);
}
